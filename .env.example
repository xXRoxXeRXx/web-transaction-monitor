# Environment Configuration Template
# Copy this file to .env and fill in your actual credentials

# Scheduling Configuration
SCHEDULE_INTERVAL=300
PROMETHEUS_PORT=8000

# IONOS Storage Credentials
IONOS_USER=your_ionos_username
IONOS_PASS=your_ionos_password

# Kallisto Workspace Credentials
KALLISTO_USER=your_kallisto_username
KALLISTO_PASS=your_kallisto_password

# Playwright Configuration (optional)
HEADLESS=true

# Add additional credentials here as needed
# For new services, follow the pattern:
# SERVICE_NAME_USER=username
# SERVICE_NAME_PASS=password
